class Cat {
	protected int age = 13;
	public void barking() {
		System.out.println("Meow...meow");
	}
	public void showGender(char gender) {
		String genderLabel;
	switch(gender) {
		case 'M' :{
		 System.out.println("He is MALE");
	}break;
		case 'F' :{
		 System.out.println("She is a FEMALE");
	}break;
	default:
		 System.out.println("Non binary animal");
		}
	}
}





