import java.util.Scanner;
public class Latihan08 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		int npm = input.nextInt();
		String fullname = input.nextLine();
		String classname = input.nextLine();
		float ipk = input.nextFloat();
		
		System.out.print("Masukan NPM Anda : " + npm);
		System.out.println("Masukan Nama Lengkap : " + fullname);
		System.out.println("Masukan Kelas Anda : " +classname);
		System.out.println("Masukan IPK Anda :" +ipk);

		input.close();
	}

}
